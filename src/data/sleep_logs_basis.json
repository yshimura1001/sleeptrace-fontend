{
  "table_name": "sleep_logs",
  "columns":[
    {
      "name": "id",
      "type": "int",
      "primary_key": true
    },
    {
      "name": "sleep_date",
      "type": "text"
    },
    {
      "name": "sleep_score",
      "type": "int"
    },
    {
      "name": "bed_time",
      "type": "TEXT"
    },
    {
      "name": "wakeup_time",
      "type": "TEXT",
      "convert": [
        {
          "str_format": "HH:MM"
        }
      ]
    },
    {
      "name": "wakeup_count",
      "type": "int",
      "basis": [
        {
          "conditions": "<=1",
          "value": "good",
          "else": "bad",
          "upper_limit_color": "red"
        }
      ]
    },
    {
      "name": "deep_sleep_continuity",
      "type": "int",
      "basis": [
        {
          "conditions": ">=70",
          "value": "good",
          "else": "bad",
          "lower_limit_color": "red"
        }
      ]
    },
    {
      "name": "sleep_duration",
      "type": "int",
      "basis": [
        {
          "conditions": "<=10:00",
          "value": "good",
          "else": "bad",
          "upper_limit_color": "red"
        }
      ]
    },
    {
      "name": "deep_sleep_percentage",
      "type": "int",
      "convert": [
        {
          "str_replace": {
            "from": "%",
            "to": ""
          }
        }
      ],
      "basis": [
        {
          "conditions": ">=20 && <=60",
          "value": "good",
          "else": "bad",
          "lower_limit_color": "blue",
          "upper_limit_color": "red"
        }
      ]
    },
    {
      "name": "light_sleep_percentage",
      "type": "int",
      "convert": [
        {
          "str_replace": {
            "from": "%",
            "to": ""
          }
        }
      ],
      "basis": [
        {
          "conditions": "<55",
          "value": "good",
          "else": "bad",
          "upper_limit_color": "red"
        }
      ]
    },
    {
      "name": "rem_sleep_percentage",
      "type": "int",
      "convert": [
        {
          "str_replace": {
            "from": "%",
            "to": ""
          }
        }
      ],
      "basis": [
        {
          "conditions": ">=10 && <=30",
          "value": "good",
          "else": "bad",
          "lower_limit_color": "blue",
          "upper_limit_color": "red"
        }
      ]
    },
    {
      "name": "sum_percentage",
      "type": "int",
      "omit": true
    }
  ]
}